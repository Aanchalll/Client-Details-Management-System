<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
<servlet>
<servlet-name>WelcomeServlet</servlet-name>
<servlet-class>com.demo.controller.Login</servlet-class>
</servlet>
<servlet>
<servlet-name>RegisterServlet</servlet-name>
<servlet-class>com.demo.controller.Register</servlet-class>
</servlet>
<servlet>
<servlet-name>NewClientServlet</servlet-name>
<servlet-class>com.demo.controller.NewClient</servlet-class>
</servlet>
<servlet>
<servlet-name>ClientList</servlet-name>
<servlet-class>com.demo.controller.ClientList</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>WelcomeServlet</servlet-name>
<url-pattern>/login.do</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>RegisterServlet</servlet-name>
<url-pattern>/register.do</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>NewClientServlet</servlet-name>
<url-pattern>/newclient.do</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>ClientList</servlet-name>
<url-pattern>/clientlist.do</url-pattern>
</servlet-mapping>
<welcome-file-list>
<welcome-file>Welcome.jsp</welcome-file>
</welcome-file-list>
</web-app>
